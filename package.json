{
  "name": "paperfit",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 5173",
    "start:api": "node services/api/server.js",
    "test:unit": "vitest run test/unit",
    "test:integration": "vitest run test/integration",
    "test:contract": "echo 'Contract validation skipped (requires external tool)'",
    "test:e2e:web": "playwright test",
    "stack:test:up": "docker compose -f infra/compose/docker-compose.test.yml up -d --build",
    "stack:test:down": "docker compose -f infra/compose/docker-compose.test.yml down -v",
    "ci:test": "npm run test:unit && npm run test:integration && npm run test:e2e:web"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "echarts": "^6.0.0",
    "echarts-for-react": "^3.0.5",
    "express": "^5.2.1",
    "framer-motion": "^12.26.1",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.12.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^22.14.0",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^5.0.0",
    "jsdom": "^27.4.0",
    "supertest": "^7.2.2",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^4.0.17"
  }
}
